<h3>Paginator</h3>
<div class="code"><code>@("<ListPaginator BatchSize=\"25\" ResultCount=\"126\" @bind-Value=\"_currentPage\"/>")</code></div>

<div class="d-flex">
	<label>Current Page: </label>
	<EditForm Model="_resultCount">
		<InputNumber @bind-Value="_resultCount"></InputNumber>
	</EditForm>
	<span class="ms-2">@(_currentPage + 1)</span>
</div>
<ListPaginator BatchSize="25" ResultCount="_resultCount" @bind-Value="_currentPage" OnChange="((s) => Console.WriteLine(s))"></ListPaginator>
