@code {
	#region Parameters
	/// <summary>
	/// IconType enumeration requesting a specific
	/// </summary>
	[Parameter]
	public s.IconType I { get; set; } // Keep parameter short since we only have one...

	/// <summary>
	/// Used for the icon's title text
	/// </summary>
	[Parameter]
	public string AltText { get; set; }

	[Parameter]
	public string CustomIcon { get; set; }

	[Parameter]
	public string AdditionalClasses { get; set; }
	#endregion

	#region Properties
	private string customIconString;
	#endregion

	protected override void OnParametersSet()
	{
		if (!string.IsNullOrEmpty(CustomIcon))
		{
			customIconString = "fas " + CustomIcon;
		}
	}
}
