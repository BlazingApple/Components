@using BlazingApple.Components.Services;
@inherits BlazingApple.Components.Icon

@switch (I)
{
	case IconType.CSpan:
		<img title="@AltText" class="@AdditionalClasses" src="https://static.c-span.org/assets/images/CSPAN_Navy-and-Blue.svg" />
		break;
	case IconType.GovTrack:
		<img title="@AltText" class="@AdditionalClasses" src="https://www.govtrack.us/static/images/logo.png?201906" @attributes="AdditionalAttributes" />
		break;
	case IconType.OurGov:
		<svg title="@AltText" class="@AdditionalClasses og-fill-primary branded" height="40" width="40" @attributes="AdditionalAttributes">
			<!--<use href="/images/OurGov Logo - Rounded Rectangle.svg#roundedRectIcon"></use>-->
			<use href="/images/OurGov Logo - Rounded.svg#roundIcon"></use>
		</svg>
		break;
	case IconType.SealHouse:
		<svg title="@AltText" viewBox="0 0 420.20125 648.03126" class="@AdditionalClasses" @attributes="AdditionalAttributes">
			<use xlink:href="/images/organizations/government/usa/legislative/seal_us_house.svg"></use>
		</svg>
		break;
	case IconType.SealPresident:
		<svg title="@AltText" viewBox="0 0 1000 999.999" class="@AdditionalClasses" @attributes="AdditionalAttributes">
			<use xlink:href="/images/organizations/government/usa/executive/seal_office_president.svg"></use>
		</svg>
		break;
	case IconType.SealSenate:
		<svg title="@AltText" viewBox="0 0 420.28283 648.15706" class="@AdditionalClasses" @attributes="AdditionalAttributes">
			<use xlink:href="/images/organizations/government/usa/legislative/seal_us_senate.svg"></use>
		</svg>
		break;
}