@Response

@code {
	#region Properties
	private static readonly string[] responses = {
		"Stored away in our vaults!",
		"Filed away for safekeeping."
	};

	private string Response
	{
		get
		{
			return ResponseSuccess.GetResponse();
		}
	}
	#endregion

	public static string GetResponse()
	{
		int count = responses.Length;
		return responses[new Random().Next(0, count)];
	}
}
